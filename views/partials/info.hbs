{{#if data}}
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Clave</th>
                <th scope="col">Valor</th>

            </tr>
        </thead>
        <tbody>
            {{#each data}}
                <tr>
                    <th scope="row">{{@key}}</th>

                    <td>{{this}}</td>

                </tr>
            {{/each}}
        </tbody>
    </table>
{{/if}}